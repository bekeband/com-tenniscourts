<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset>
		<field
				name="id"
				type="text"
				label="TENNISFEATURE_ID_FIELD"
				description="TENNISFEATURE_ID_DESC"
				size="60"
				class="inputbox"
				default=""
				/>
		<field
				name="single_play"
				type="checkbox"
				label="TENNISFEATURE_SINGLE_PLAY_FIELD"
				description="TENNISFEATURE_SINGLE_PLAY_FIELD_DESC"
				size="11"
				class="inputbox"
				default=""
				/>
		<field
				name="double_play"
				type="checkbox"
				label="TENNISFEATURE_DOUBLE_PLAY_FIELD"
				description="TENNISFEATURE_DOUBLE_PLAY_FIELD_DESC"
				size="11"
				class="inputbox"
				default=""
				/>
		<field
				name="practicing"
				type="checkbox"
				label="TENNISFEATURE_PRACTICING_FIELD"
				description="TENNISFEATURE_PRACTICING_FIELD_DESC"
				size="11"
				class="inputbox"
				default=""
				/>
		<field
				name="medium"
				type="checkbox"
				label="TENNISFEATURE_MEDIUM_FIELD"
				description="TENNISFEATURE_MEDIUM_FIELD_DESC"
				size="11"
				class="inputbox"
				default=""
				/>
		<field
				name="competition"
				type="checkbox"
				label="TENNISFEATURE_COMPETITION_FIELD"
				description="TENNISFEATURE_COMPETITION_FIELD_DESC"
				size="11"
				class="inputbox"
				default=""
				/>
       <field name="prize_mult"
               active="true"
               type="meter"
               label="TENNISFEATURE_PRIZEMULT_FIELD"
				description="TENNISFEATURE_PRIZEMULT_FIELD_DESC"
               max="2"
               min="0.5"
               step="0.05"
               default="1"
               />		
	</fieldset>
</form>